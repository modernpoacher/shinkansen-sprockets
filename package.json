{
  "name": "shinkansen-sprockets",
  "version": "1.1.173",
  "description": "Shinkansen Sprockets",
  "keywords": [
    "Shinkansen",
    "Sprockets",
    "Zashiki Karakuri",
    "Zashiki",
    "Karakuri",
    "React"
  ],
  "main": "./src/index.cjs",
  "type": "module",
  "types": "./src/index.d.cts",
  "author": {
    "name": "Jonathan Perry for Modern Poacher Limited",
    "email": "modernpoacher@modernpoacher.com",
    "url": "https://modernpoacher.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.12.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/modernpoacher/shinkansen-sprockets.git"
  },
  "scripts": {
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "build": "cross-env DEBUG=shinkansen-sprockets npm run gulp -- build",
    "build:watch": "cross-env DEBUG=shinkansen-sprockets npm run gulp -- build:watch",
    "transform": "cross-env DEBUG=shinkansen-sprockets npm run gulp -- transform",
    "transform:watch": "cross-env DEBUG=shinkansen-sprockets npm run gulp -- transform:watch",
    "pregulp": "npm run nvm",
    "gulp": "gulp --color",
    "prelint": "npm run nvm",
    "lint": "cross-env NODE_ENV=production eslint . .storybook --ext .mjs,.cjs,.jsx,.mts,.cts",
    "lint:fix": "npm run lint -- --fix",
    "pretest": "npm run nvm",
    "test": "cross-env NODE_ENV=test jest --collect-coverage false --run-in-band",
    "nvm": "bash nvm.sh",
    "prestorybook": "npm run build && npm run transform",
    "storybook": "storybook dev -p 6007",
    "prepare": "husky install",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "debug": "^4.3.4",
    "fast-deep-equal": "^3.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.23.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@babel/register": "^7.22.15",
    "@modernpoacher/design-system": "1.0.109",
    "@modernpoacher/hooks": "^1.0.451",
    "@sequencemedia/gulp": "^1.1.124",
    "@sequencemedia/gulp-clean-css": "^1.0.61",
    "@sequencemedia/gulp-css-purge": "^1.1.57",
    "@sequencemedia/gulp-debug": "^1.0.65",
    "@sequencemedia/gulp-postcss": "^1.0.68",
    "@sequencemedia/gulp-sass": "^1.0.60",
    "@sequencemedia/gulp-sourcemaps": "^1.0.63",
    "@storybook/addon-actions": "^7.6.3",
    "@storybook/addon-essentials": "^7.6.3",
    "@storybook/addon-links": "^7.6.3",
    "@storybook/react": "^7.6.3",
    "@storybook/react-webpack5": "^7.6.3",
    "@types/react": "^18.2.42",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "autoprefixer": "^10.4.16",
    "babel-plugin-module-resolver": "^5.0.0",
    "core-js": "^3.34.0",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "del": "^7.1.0",
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-react": "^7.33.2",
    "glob-all": "^3.3.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "postcss": "^8.4.32",
    "postcss-easy-import": "^4.0.0",
    "postcss-map": "^0.11.0",
    "postcss-normalize": "^10.0.1",
    "postcss-scss": "^4.0.9",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.20.1",
    "react-router-dom": "^6.20.1",
    "react-test-renderer": "^18.2.0",
    "regenerator-runtime": "^0.14.0",
    "sass": "^1.69.5",
    "storybook": "^7.6.3",
    "vinyl-paths": "^5.0.0"
  },
  "imports": {
    "#build/gulp/build/*": "./build/gulp/build/*.mjs",
    "#build/gulp/build": "./build/gulp/build/index.mjs",
    "#build/gulp/*": "./build/gulp/*.mjs",
    "#build/gulp/transform": "./build/gulp/transform/index.mjs",
    "#build/paths": "./build/paths.mjs"
  },
  "exports": {
    ".": {
      "require": "./src/index.cjs",
      "import": "./src/index.mjs"
    },
    "./sprockets/check-answers/group/*": "./src/sprockets/check-answers/group/*.cjs",
    "./sprockets/check-answers/group": "./src/sprockets/check-answers/group/index.cjs",
    "./sprockets/check-answers/title": "./src/sprockets/check-answers/title/index.cjs",
    "./sprockets/check-answers": "./src/sprockets/check-answers/index.cjs",
    "./sprockets/error-summary/group/*": "./src/sprockets/error-summary/group/*.cjs",
    "./sprockets/error-summary/group": "./src/sprockets/error-summary/group/index.cjs",
    "./sprockets/error-summary/title": "./src/sprockets/error-summary/title/index.cjs",
    "./sprockets/error-summary": "./src/sprockets/error-summary/index.cjs",
    "./sprockets/fieldset/description": "./src/sprockets/fieldset/description/index.cjs",
    "./sprockets/fieldset/error-message": "./src/sprockets/fieldset/error-message/index.cjs",
    "./sprockets/fieldset/group": "./src/sprockets/fieldset/group/index.cjs",
    "./sprockets/fieldset/title": "./src/sprockets/fieldset/title/index.cjs",
    "./sprockets/fieldset": "./src/sprockets/fieldset/index.cjs",
    "./sprockets": "./src/sprockets/index.cjs",
    "./components/common/text-content": "./src/components/common/text-content/index.cjs",
    "./components/common/visually-hidden-text": "./src/components/common/visually-hidden-text/index.cjs",
    "./components/description": "./src/components/description/index.cjs",
    "./components/error-message": "./src/components/error-message/index.cjs",
    "./components/group": "./src/components/group/index.cjs",
    "./components/title": "./src/components/title/index.cjs",
    "./transformers/check-answers": "./src/transformers/check-answers/index.mjs",
    "./transformers/common": "./src/transformers/common/index.mjs",
    "./transformers/error-message": "./src/transformers/error-message/index.mjs",
    "./transformers/error-summary": "./src/transformers/error-summary/index.mjs"
  }
}
