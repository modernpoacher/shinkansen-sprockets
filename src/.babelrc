{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "node": "current",
          "browsers": [
            "last 4 versions",
            "safari >= 9",
            "ios >= 8",
            "ie >= 9",
            "> 2%"
          ]
        },
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    "@babel/syntax-jsx",
    "@babel/proposal-export-default-from",
    "@babel/proposal-export-namespace-from",
    [
      "@babel/proposal-class-properties",
      {
        "loose": false
      }
    ],
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "alias": {
          "shinkansen-sprockets/sprockets/check-answers": "./sprockets/check-answers/index.jsx",
          "shinkansen-sprockets/sprockets/error-summary": "./sprockets/error-summary/index.jsx",
          "shinkansen-sprockets/sprockets/fieldset": "./sprockets/fieldset/index.jsx",
          "shinkansen-sprockets/sprockets": "./sprockets/index.jsx",
          "shinkansen-sprockets/components/common/text-content": "./components/common/text-content/index.jsx",
          "shinkansen-sprockets/components/description/fieldset": "./components/description/fieldset/index.jsx",
          "shinkansen-sprockets/components/description": "./components/description/index.jsx",
          "shinkansen-sprockets/components/error-message/fieldset": "./components/error-message/fieldset/index.jsx",
          "shinkansen-sprockets/components/error-message": "./components/error-message/index.jsx",
          "shinkansen-sprockets/components/group/check-answers/answer-title": "./components/group/check-answers/answer-title.jsx",
          "shinkansen-sprockets/components/group/check-answers/answer-value": "./components/group/check-answers/answer-value.jsx",
          "shinkansen-sprockets/components/group/check-answers/change-answer": "./components/group/check-answers/change-answer.jsx",
          "shinkansen-sprockets/components/group/check-answers/visually-hidden-text": "./components/group/check-answers/visually-hidden-text.jsx",
          "shinkansen-sprockets/components/group/check-answers": "./components/group/check-answers/index.jsx",
          "shinkansen-sprockets/components/group/error-summary": "./components/group/error-summary/index.jsx",
          "shinkansen-sprockets/components/group/fieldset": "./components/group/fieldset/index.jsx",
          "shinkansen-sprockets/components/group": "./components/group/index.jsx",
          "shinkansen-sprockets/components/title/check-answers": "./components/title/check-answers/index.jsx",
          "shinkansen-sprockets/components/title/error-summary": "./components/title/error-summary/index.jsx",
          "shinkansen-sprockets/components/title/fieldset": "./components/title/fieldset/index.jsx",
          "shinkansen-sprockets/components/title": "./components/title/index.jsx",
          "shinkansen-sprockets/transformers/check-answers": "./transformers/check-answers/index.mjs",
          "shinkansen-sprockets/transformers/common": "./transformers/common/index.mjs",
          "shinkansen-sprockets/transformers/error-message": "./transformers/error-message/index.mjs",
          "shinkansen-sprockets/transformers/error-summary": "./transformers/error-summary/index.mjs",
          "build": "../build",
          "stories": "../stories"
        }
      }
    ]
  ]
}
